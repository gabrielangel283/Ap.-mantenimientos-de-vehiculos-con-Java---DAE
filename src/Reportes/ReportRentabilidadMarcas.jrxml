	<queryString>
		<![CDATA[SELECT 
            m.nombre_marca,
            COUNT(c.comprobante_id) as cantidad_atenciones,
            SUM(c.monto_total) as total_facturado
        FROM MARCA m
        INNER JOIN VEHICULO v ON m.marca_id = v.marca_id
        INNER JOIN ORDEN o ON v.vehiculo_id = o.vehiculo_id
        INNER JOIN COMPROBANTE c ON o.orden_id = c.orden_id
        WHERE c.fecha_pago BETWEEN $P{fecha_ini} AND $P{fecha_fin}
        GROUP BY m.nombre_marca
        ORDER BY total_facturado DESC
        LIMIT 15]]>
	</queryString>

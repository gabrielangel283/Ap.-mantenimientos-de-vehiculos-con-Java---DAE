	<queryString>
		<![CDATA[SELECT 
    s.servicio_id,
    s.nombre_servicio,
    COUNT(ds.orden_id) AS cantidad_ventas,
    SUM(ds.precio) AS total_generado,
    AVG(ds.precio) AS precio_promedio,
    MIN(ds.precio) AS precio_minimo,
    MAX(ds.precio) AS precio_maximo
FROM SERVICIO s
INNER JOIN DETALLE_SERVICIO ds 
	ON s.servicio_id = ds.servicio_id
INNER JOIN  ORDEN o 
	ON ds.orden_id = o.orden_id
WHERE   o.estado IN ('completado', 'pagado') 
	AND o.fecha_inicio BETWEEN $P{fecha_ini} AND $P{fecha_fin}
GROUP BY   s.servicio_id, s.nombre_servicio
ORDER BY   total_generado DESC
LIMIT 10;]]>
	</queryString>

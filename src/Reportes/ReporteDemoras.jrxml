	<queryString>
		<![CDATA[SELECT 
            v.num_placa,
            m.nombre_marca || ' ' || v.modelo as vehiculo,
            c.nombres || ' ' || COALESCE(c.apellidos, '') as cliente,
            u.nombres as mecanico,
            o.fecha_inicio,
            (CURRENT_DATE - o.fecha_inicio) as dias_transcurridos
        FROM ORDEN o
        INNER JOIN VEHICULO v ON o.vehiculo_id = v.vehiculo_id
        INNER JOIN MARCA m ON v.marca_id = m.marca_id
        INNER JOIN CLIENTE c ON v.cliente_id = c.cliente_id
        LEFT JOIN USUARIO u ON o.usuario_id = u.usuario_id
        WHERE o.estado = $P{p_estado}
        ORDER BY dias_transcurridos DESC]]>
	</queryString>

	<subDataset name="Cant_Comprobantes" uuid="bb848674-a634-4069-8dd1-ce7674f433de">
		<queryString language="SQL">
			<![CDATA[SELECT tipo_comprobante, COUNT(comprobante_id) AS cantidad_emitida
FROM COMPROBANTE
GROUP BY tipo_comprobante;]]>
		</queryString>
		<field name="tipo_comprobante" class="java.lang.String"/>
		<field name="cantidad_emitida" class="java.lang.Long"/>
	</subDataset>
	<subDataset name="Ingresos_tipoCliente" uuid="967ddd69-36d1-49c3-aa49-3555be9ac984">
		<queryString language="SQL">
			<![CDATA[SELECT
    C.tipo AS tipo_cliente,
    SUM(CP.monto_total) AS ingresos_totales
FROM
    CLIENTE C
JOIN
    COMPROBANTE CP ON C.cliente_id = CP.cliente_id
GROUP BY
    C.tipo
ORDER BY
    ingresos_totales DESC;]]>
		</queryString>
		<field name="tipo_cliente" class="java.lang.String"/>
		<field name="ingresos_totales" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Ingresos_tipo_cliente" uuid="d5f4de80-f645-4493-93dd-f99a49febfcd">
		<queryString language="SQL">
			<![CDATA[SELECT
    C.tipo AS tipo_cliente,
    SUM(CP.monto_total) AS ingresos_totales
FROM
    CLIENTE C
JOIN
    COMPROBANTE CP ON C.cliente_id = CP.cliente_id
GROUP BY
    C.tipo
ORDER BY
    ingresos_totales DESC;]]>
		</queryString>
		<field name="tipo_cliente" class="java.lang.String"/>
		<field name="ingresos_totales" class="java.math.BigDecimal"/>

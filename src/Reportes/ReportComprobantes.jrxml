	<subDataset name="Gepeto_noesclave" uuid="7770aa57-ca59-4125-b8ff-ce50ef164495">
		<queryString language="SQL">
			<![CDATA[SELECT
    tipo_comprobante,
    SUM(monto_total) AS ingresos_totales
FROM
    COMPROBANTE
GROUP BY
    tipo_comprobante
ORDER BY
    ingresos_totales DESC;]]>
		</queryString>
		<field name="tipo_comprobante" class="java.lang.String"/>
		<field name="ingresos_totales" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="REPORT_DIR" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
    CASE
        WHEN C.tipo = 'natural' THEN C.nombres || ' ' || COALESCE(C.apellidos, '')
        ELSE C.nombres
    END AS nombre_cliente,
    C.tipo AS tipo_cliente,
    SUM(CP.monto_total) AS monto_total_vendido
FROM
    CLIENTE C
JOIN
    COMPROBANTE CP ON C.cliente_id = CP.cliente_id
GROUP BY
    C.cliente_id, C.nombres, C.apellidos, C.tipo
ORDER BY
    monto_total_vendido DESC
LIMIT 7;]]>
	</queryString>
	<field name="nombre_cliente" class="java.lang.String"/>
	<field name="tipo_cliente" class="java.lang.String"/>
	<field name="monto_total_vendido" class="java.math.BigDecimal"/>
